{
    "expo": {
        "name": "Estimly",
        "slug": "estimly",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "newArchEnabled": true,
        "splash": {
            "resizeMode": "contain",
            "backgroundColor": "#2563EB"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.hunter.estimly"
        },
        "android": { "package": "com.hunter.estimly" },
        "web": {
            "bundler": "metro"
        },
        "plugins": ["expo-router"],
        "scheme": "estimly"
    }
}
